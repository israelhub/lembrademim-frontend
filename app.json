{
  "expo": {
    "name": "lembrademimapp2",
    "slug": "lembrademimapp2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/applogo.png",
    "scheme": "lembrademimapp2",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.lembrademimapp2.app",
      "supportsTablet": true
    },
    "android": {
      "package": "com.lembrademimapp2.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/applogo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/applogo.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/applogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "946d6ee5-1241-4f0b-be2e-c6a5f37f295b"
      }
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "https": false
    }
  }
}
